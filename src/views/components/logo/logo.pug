mixin logo(props = {})
  - const rootClassName = "logo";
  - const className = props.class || null;
  - const style = props.style || null;
  - const link = props.link || "/";
  -
    const classes = [
      rootClassName,
      className
    ];

  a(
    class = classes 
    style = style
    href = link
  ) #{appData.name}